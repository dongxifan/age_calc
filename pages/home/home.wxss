.content {
  width: 100%;
  height: 100%;
}

.content .choose-title {
  margin: 0 auto;
  margin-top: 40vh;
  font-size: 36rpx;
  text-align: center;
}

.content .date {
  margin: 0 auto;
  margin-bottom: 10vh;
  margin-top: 1vh;
  width: 350rpx;
  font-size: 48rpx;
  text-align: center;
  border-bottom: 1rpx solid rgba(134, 133, 133);

}

.content .start-btn {
  width: 500rpx;
  margin: 0 auto;
  text-align: center;
  display: block;
}

.custom-class {
  font-size: 12rpx;
}

.date-panel {
  background-color: white;
}

.van-picker__cancel,
.van-picker__confirm {
  font-size: 32rpx !important;
}

.date-footer {
  height: 180rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.date-btn button{
  height: 70rpx;
  border-radius: 10rpx;
}

